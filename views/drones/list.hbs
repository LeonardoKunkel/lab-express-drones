<h2>Available drones</h2>
<hr>

{{#each data}}
<div>
    <h3>{{ name }}</h3>
    <p>{{ propellers }}</p>
    <p>{{ maxSpeed }}</p>
    <a href="/drones/{{_id}}/update-form">Editar</a>
    <form action="/drones/{{_id}}/delete" method="post">
        <button>Borrar</button>
    </form>
</div>
{{/each}}
